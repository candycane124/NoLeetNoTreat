{
    "manifest_version": 3,
    "name": "NoLeetNoTreat",
    "description": "Stop your impulse buying & level up your coding!",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "placeholder.png"
    },
    "permissions": ["activeTab"], 
    "web_accessible_resources": [
    {
      "resources": ["assets/slogan.gif"],
      "matches": ["<all_urls>"]
    }
  ],
    "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
  }
